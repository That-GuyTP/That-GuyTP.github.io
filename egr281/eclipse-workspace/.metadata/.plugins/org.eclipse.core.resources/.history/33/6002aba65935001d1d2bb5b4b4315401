package edu.mtc.egr281.chapter03;

import java.util.Scanner;

public class SimpleNestedIf {

	public static void main(String[] args) {
		
		//Declaration
		Scanner kb = new Scanner(System.in);
		
		System.out.print("Do you want to see a movie? ");
		
		//Close Scanner
		kb.close();
		
		
	}//EOC main
	
}//EOC SimpleNestedIf
